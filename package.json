{
  "name": "chat-query",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "db:migrate": "npx tsc -p tsconfig.migrations.json && npx sequelize-cli db:migrate",
    "db:gen-migrate": "npx tsc -p tsconfig.migrations.json && npx sequelize-cli db:seed:all",
    "db:auto-migrate": "npx tsc -p tsconfig.migrations.json && npx makemigration --name test1",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch --path tsconfig.dev.json",
    "start:prod": "node dist/main",
    "build-image": "npm run build && docker build -t jadenxiong/chat-query-backend:latest .",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "sh node_modules/.bin/jest --runInBand --config ./test/jest-e2e.json --watch --debug",
    "test:e2e": "jest --config ./test/jest-e2e.json --watch --debug"
  },
  "dependencies": {
    "@auth/express": "^0.8.0",
    "@auth/sequelize-adapter": "^2.7.0",
    "@google/generative-ai": "^0.1.3",
    "@langchain/core": "latest",
    "@langchain/openai": "latest",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/sequelize": "^9.0.2",
    "@types/sequelize": "^4.28.15",
    "@vercel/fun": "^1.1.1",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "best-effort-json-parser": "^1.1.2",
    "body-parser": "^1.20.2",
    "dbgate-query-splitter": "^4.10.3",
    "dbml-core": "latest",
    "diff": "^7.0.0",
    "dotenv": "^16.1.4",
    "finale-rest": "^1.2.2",
    "fnmatch": "^1.0.2",
    "knex": "^2.4.2",
    "knex-utils": "^6.0.0",
    "langchain": "latest",
    "lodash": "^4.17.21",
    "mysql2": "^3.3.3",
    "openai": "^4.47.1",
    "oracledb": "^6.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sequelize": "^6.37.3",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.5",
    "typeorm": "^0.3.16",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "resolutions": {
    "sequelize": "^6.32.0",
    "oracledb": "^6.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "peerDependenciesMeta": {
    "langchain": {
      "injected": true
    }
  },
  "dependenciesMeta": {}
}